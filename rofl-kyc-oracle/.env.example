# ROFL KYC Oracle Configuration
# ==============================
# Copy this file to .env and fill in your actual values

# ROFL Oracle private key (with 0x prefix)
ROFL_PRIVATE_KEY=0xyour_private_key_here

# Sapphire Testnet RPC endpoint
SAPPHIRE_RPC=https://testnet.sapphire.oasis.io

# Deployed KYC Registry contract address
KYC_REGISTRY_ADDRESS=0xyour_contract_address_here

# Enable Falcon-512 signatures (currently not implemented)
ENABLE_FALCON=false

# Logging level
RUST_LOG=info
